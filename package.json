{
  "name": "redux-intl-connect",
  "version": "1.1.1",
  "description": "redux connect agnostic bindings for i18n with FormatJS formatMessage inspired API",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "deploy-gh-pages": "cd examples && yarn run deploy",
    "clean": "rimraf lib coverage .nyc_output",
    "lint": "eslint --ext js src tests",
    "prepare": "npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha 'tests/**/*.spec.@(js|jsx)'",
    "posttest": "npm run lint",
    "coverage": "cross-env NODE_ENV=test nyc report --reporter=lcov",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeojz/redux-intl-connect.git"
  },
  "keywords": [
    "translation",
    "translate",
    "formatting",
    "format",
    "reactjs",
    "preact",
    "react",
    "globalization",
    "localization",
    "local",
    "internationalization",
    "i18n",
    "intl"
  ],
  "author": "Gerald Yeo <gerald@fusedthought.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/yeojz/redux-intl-connect/issues"
  },
  "homepage": "https://github.com/yeojz/redux-intl-connect#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.4",
    "eslint": "^3.13.1",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "lodash": "^4.17.4"
  }
}
