{
  "name": "redux-intl-connect",
  "version": "1.0.0",
  "description": "redux connect agnostic bindings for i18n with FormatJS formatMessage compatible API",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "rimraf lib coverage",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-register 'tests/**/*.spec.@(js|jsx)'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeojz/redux-intl-connect.git"
  },
  "keywords": [
    "translation",
    "translate",
    "formatting",
    "format",
    "reactjs",
    "preact",
    "react",
    "globalization",
    "localization",
    "local",
    "internationalization",
    "i18n",
    "intl"
  ],
  "author": "Gerald Yeo <gerald@fusedthought.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/yeojz/redux-intl-connect/issues"
  },
  "homepage": "https://github.com/yeojz/redux-intl-connect#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "redux": "^3.6.0"
  }
}
